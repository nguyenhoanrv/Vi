PROGRAM  EXAMPLE3;  (* TOWER OF HANOI *)
VAR  I:INTEGER;  /* abcd /* abcd 
     N:INTEGER;  
     P:INTEGER;  
     Q:INTEGER;
     C:CHAR; //12344


BEGIN
  FOR  N := 1  TO  4  DO  
    BEGIN
      FOR  I:=1  TO  4  DO  
        CALL  WRITEC(' ');
      CALL  READC(C);  
      CALL  WRITEC(C)
    END;
  P:=1;  
  Q:=2;
  FOR  N:=2  TO  4  DO
    BEGIN  
      I:=0;  
      CALL  HANOI(N,P,Q);  
      CALL  WRITELN  
    END
END.  (* TOWER OF HANOI *)